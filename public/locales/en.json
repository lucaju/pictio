    "challenges": {
        "page": {
            "title": "Pick a challenge",
            "draw": "Draw",
            "ready": "I'm ready",
            "back": "Back"
        },
        "challenges": {
           "wall": {
              "name": "Drawing on the Wall",
              "description":"In this challenge, the two human players work together to produce one sketch representing the word given by the tablet. The player who saw the word must draw on a wall, using just their fingers, without leaving any visible mark. The second human acts as a proxy, following the trajectory of the finger and trying to reproduce it on the screen of the tablet. The AI program has one minute to guess what is being drawn."
            },
            "blind": {
              "name": "Blind Drawing with Left/Right Hand",
              "description":"This is an individual challenge, where the drawer sketch representations for the word selected without looking at the screen and using her/his non-dominant hand. While the human player draws, the machine will attempt to guess what is being drawn."
            },
            "verbal": {
              "name": "Verbal Description",
              "description":"In this challenge, the two human players work together to produce one sketch representing the word selected. The player who saw the word must verbally describe it, using only geometrical figures and spatial orientation. The second human acts as a proxy, following the instructions and trying to reproduce it on the screen of the tablets. The AI program has one minute to guess what is being drawn."
            }
        }
    },
